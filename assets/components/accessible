//Lists all accessible components. 
component hull:
    cost: METAL: 500 //Changes the metal cost to 500
    cost: SPACE: -5 //Changes the space cost to -5 (gain 5 space)
    cost: MASS: -3 //Changes the mass cost to -3 (gain 3 mass)
    cost: PRODUCTION: 3 //Changes the production cost to 3
